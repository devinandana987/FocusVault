{
  "manifest_version": 3,
  "name": "FocusVault",
  "version": "1.0",
  "description": "Track study time and save highlights",

  "permissions": ["storage", "activeTab", "scripting"],

  "background": {
    "service_worker": "background/background.js"
  },

  "action": {
    "default_popup": "index/index.html"
    
  },

  "content_scripts": [
    {
      
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
